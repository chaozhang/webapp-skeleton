{
    "name": "webapp-skeleton-react-redux",
    "version": "1.0.0",
    "description": "skeleton for a simple web application, React+Redux+webpack+stylus+react-router4",
    "main": "index.js",
    "scripts": {
        "build": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --mode development --config webpack.config.js --watch",
        "export": "./node_modules/.bin/webpack --mode development --config webpack.config.export.js",
        "start": "webpack-dev-server --mode development",
        "deploy": "gh-pages -d www"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chaozhang/webapp-skeleton-react-redux.git"
    },
    "author": "czhang",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/chaozhang/webapp-skeleton-react-redux/issues"
    },
    "homepage": "https://github.com/chaozhang/webapp-skeleton-react-redux#readme",
    "dependencies": {
        "ChaoWebCommon": "chaozhang/ChaoWebCommon",
        "prop-types": "^15.6.1",
        "react": "^16.3.1",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.0",
        "reselect": "^3.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "gh-pages": "^1.1.0",
        "nib": "^1.1.2",
        "redux-devtools-extension": "^2.13.2",
        "style-loader": "^0.20.3",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "webpack": "^4.5.0",
        "webpack-cli": "^2.0.14",
        "webpack-dev-server": "^3.1.3"
    },
    "babel": {
        "presets": [
            "env",
            "react"
        ]
    }
}
